{
  "name": "next-p5-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier -l \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "prettier:fix": "yarn prettier --write",
    "format": "yarn lint && yarn prettier",
